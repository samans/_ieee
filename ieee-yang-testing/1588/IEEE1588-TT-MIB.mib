    ::= { ieee1588UnicastDiscoveryPortDsEntry 3 }
-- .2.15.7.1.1.3

ieee1588UnicastDiscoveryPortDsLogQueryInterval OBJECT-TYPE
    SYNTAX       Ieee1588LogTimeInterval
    UNITS        "log2 seconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "Logarithm to the base 2 of the mean interval in seconds between
        requests from a PTP Instance for a unicast Announce message."
    REFERENCE    "IEEE 1588-2019: clause 17.4.3.3.1"
    ::= { ieee1588UnicastDiscoveryPortDsEntry 4 }
-- .2.15.7.1.1.4

ieee1588UnicastDiscoveryPortDsPortAddressTable OBJECT-TYPE
    SYNTAX    SEQUENCE OF Ieee1588UnicastDiscoveryPortDsPortAddressEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Lists protocol address records of the unicastDiscoveryPortDS
        data set for a specific PTP Port within PTP Instance. Elements
        in the list can be created or deleted, if those operations are
        supported by management."
    REFERENCE    "IEEE 1588-2019: clause 17.4.3"
    ::= { ieee1588UnicastDiscoveryPortDs 2 }
-- .2.15.7.2

ieee1588UnicastDiscoveryPortDsPortAddressEntry OBJECT-TYPE
    SYNTAX       Ieee1588UnicastDiscoveryPortDsPortAddressEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing a protocol addresses record of a
        unicastDiscoveryPortDS data set for a specific PTP Port within
        PTP Instance."
    INDEX        {
        ieee1588InstanceIndex,
        ieee1588PortIndex,
        ieee1588UnicastDiscoveryPortDsPortAddressIndex }
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressTable 1 }
-- .2.15.7.2.1


Ieee1588UnicastDiscoveryPortDsPortAddressEntry    ::= SEQUENCE {
        ieee1588UnicastDiscoveryPortDsPortAddressIndex Unsigned32,
        ieee1588UnicastDiscoveryPortDsPortRowStatus RowStatus,
        ieee1588UnicastDiscoveryPortDsPortAddressNetworkProtocol
                                             Ieee1588NetworkProtocol,
        ieee1588UnicastDiscoveryPortDsPortAddressAddressLength
                                             Unsigned32,
        ieee1588UnicastDiscoveryPortDsPortAddressAddressField
                                             PhysAddress
        }

ieee1588UnicastDiscoveryPortDsPortAddressIndex OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Table index of a protocol address in unicastDiscoveryDS data
        set for a specific PTP Port within PTP Instance."
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressEntry 3 }
-- .2.15.7.2.1.3

ieee1588UnicastDiscoveryPortDsPortRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This object is used to create and delete
        ieee1588UnicastDiscoveryPortDsPortAddressTable entries for a
        specific PTP Port within PTP Instance. All attributes shall be
        filled on row creation."
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressEntry 4 }
-- .2.15.7.2.1.4

ieee1588UnicastDiscoveryPortDsPortAddressNetworkProtocol OBJECT-TYPE
    SYNTAX       Ieee1588NetworkProtocol
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This value indicates the type of the protocol address which is
        used for unicast discovery on specific PTP Port within PTP
        Instance."
    REFERENCE    "IEEE 1588-2019: clause 17.4.3.3.3, 5.3.6"
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressEntry 5 }
-- .2.15.7.2.1.5

ieee1588UnicastDiscoveryPortDsPortAddressAddressLength OBJECT-TYPE
    SYNTAX       Unsigned32(1..16)
    UNITS        "octets"
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This value indicates the length in Octets of the address of
        protocol address which is used for unicast discovery on specific
        PTP Port within PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 17.4.3.3.3, 5.3.6"
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressEntry 6 }
-- .2.15.7.2.1.6

ieee1588UnicastDiscoveryPortDsPortAddressAddressField OBJECT-TYPE
    SYNTAX       PhysAddress(SIZE (1..16))
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This value indicates the address of the protocol address which
        is used for unicast discovery on specific PTP Port within PTP
        Instance."
    REFERENCE    "IEEE 1588-2019: clause 17.4.3.3.3, 5.3.6"
    ::= { ieee1588UnicastDiscoveryPortDsPortAddressEntry 7 }
-- .2.15.7.2.1.7

ieee1588AcceptableTimeTransmitterPortDsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ieee1588AcceptableTimeTransmitterPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The acceptableTimeTransmitterPortDS data set for a specific PTP
        Port within PTP Instance. Provides data for the optional
        Acceptable TimeTransmitter Table mechanism."
    REFERENCE    "IEEE 1588-2019: clause 17.5"
    ::= { ieee1588PortList 8 }
-- .2.15.8

ieee1588AcceptableTimeTransmitterPortDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588AcceptableTimeTransmitterPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry providing attributes of
        acceptableTimeTransmitterPortDS data set for a specific PTP Port
        within PTP Instance."
    INDEX        {
        ieee1588InstanceIndex,
        ieee1588PortIndex }
    ::= { ieee1588AcceptableTimeTransmitterPortDsTable 1 }
-- .2.15.8.1


Ieee1588AcceptableTimeTransmitterPortDsEntry    ::= SEQUENCE {
        ieee1588AcceptableTimeTransmitterPortDsEnable TruthValue
        }

ieee1588AcceptableTimeTransmitterPortDsEnable OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "When enable is false, the Acceptable TimeTransmitter table
        option is not used on this PTP Port, and the normal operation of
        the protocol is in effect.
        When enable is true, the Acceptable TimeTransmitter table option
        is used on this PTP Port as specified in the standard."
    REFERENCE    "IEEE 1588-2019: clause 17.5.4.2.1"
    ::= { ieee1588AcceptableTimeTransmitterPortDsEntry 3 }
-- .2.15.8.1.3

ieee1588L1SyncBasicPortDsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588L1SyncBasicPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The L1SyncBasicPortDS data set for a specific PTP Port within
        PTP Instance. Provides data for operation of the optional
        layer-1 based synchronization performance enhancement feature."
    REFERENCE    "IEEE 1588-2019: clause L.5"
    ::= { ieee1588PortList 9 }
-- .2.15.9

ieee1588L1SyncBasicPortDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588L1SyncBasicPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry providing attributes of L1SyncBasicPortDS data set for
        a specific PTP Port within PTP Instance."
    INDEX        {
        ieee1588InstanceIndex,
        ieee1588PortIndex }
    ::= { ieee1588L1SyncBasicPortDsTable 1 }
-- .2.15.9.1


Ieee1588L1SyncBasicPortDsEntry    ::= SEQUENCE {
        ieee1588L1SyncBasicPortDsL1SyncEnabled  TruthValue,
        ieee1588L1SyncBasicPortDsTxCoherentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsRxCoherentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsCongruentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsOptParamsEnabled TruthValue,
        ieee1588L1SyncBasicPortDsLogL1SyncInterval
                                             Ieee1588LogTimeInterval,
        ieee1588L1SyncBasicPortDsL1SyncReceiptTimeout Unsigned32,
        ieee1588L1SyncBasicPortDsL1SyncLinkAlive TruthValue,
        ieee1588L1SyncBasicPortDsIsTxCoherent   TruthValue,
        ieee1588L1SyncBasicPortDsIsRxCoherent   TruthValue,
        ieee1588L1SyncBasicPortDsIsCongruent    TruthValue,
        ieee1588L1SyncBasicPortDsL1SyncState    Ieee1588L1SyncState,
        ieee1588L1SyncBasicPortDsPeerTxCoherentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsPeerRxCoherentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsPeerCongruentIsRequired TruthValue,
        ieee1588L1SyncBasicPortDsPeerIsTxCoherent TruthValue,
        ieee1588L1SyncBasicPortDsPeerIsRxCoherent TruthValue,
        ieee1588L1SyncBasicPortDsPeerIsCongruent TruthValue
        }

ieee1588L1SyncBasicPortDsL1SyncEnabled OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "Specifies whether the L1Sync option is enabled on a specific
        PTP Port within PTP Instance. If enabled is true, then the
        L1Sync message exchange is supported and enabled."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.1, L.4.1"
    ::= { ieee1588L1SyncBasicPortDsEntry 3 }
-- .2.15.9.1.3

ieee1588L1SyncBasicPortDsTxCoherentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value specifies the configuration of the L1Sync port within
        PTP Instance and the expected configuration of its peer L1Sync
        port. This configuration indicates whether the L1Sync port is
        required to be a transmit coherent port."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.2, L.4.2"
    ::= { ieee1588L1SyncBasicPortDsEntry 4 }
-- .2.15.9.1.4

ieee1588L1SyncBasicPortDsRxCoherentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value specifies the configuration of the L1Sync port within
        PTP Instance and the expected configuration of its peer L1Sync
        port. This configuration indicates whether the L1Sync port is
        required to be a receive coherent port."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.3, L.4.3"
    ::= { ieee1588L1SyncBasicPortDsEntry 5 }
-- .2.15.9.1.5

ieee1588L1SyncBasicPortDsCongruentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value specifies the configuration of the L1Sync port within
        PTP Instance and the expected configuration of its peer L1Sync
        port. This configuration indicates whether the L1Sync port is
        required to be a congruent port."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.4, L.4.4"
    ::= { ieee1588L1SyncBasicPortDsEntry 6 }
-- .2.15.9.1.6

ieee1588L1SyncBasicPortDsOptParamsEnabled OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value specifies whether the L1Sync port within PTP Instance
        transmitting the L1_SYNC TLV extends L1_SYNC TLV with the
        information about the optional parameters defined in
        IEEE1588-2019 L.8."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.5, L.4.5, L.8"
    ::= { ieee1588L1SyncBasicPortDsEntry 7 }
-- .2.15.9.1.7

ieee1588L1SyncBasicPortDsLogL1SyncInterval OBJECT-TYPE
    SYNTAX       Ieee1588LogTimeInterval
    UNITS        "log2 seconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value is the logarithm to the base 2 of the L1SyncInterval
        which specifies the mean time interval between successive
        periodic messages sent by the L1Sync port within PTP Instance
        and carrying the L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.6, L.4.6"
    ::= { ieee1588L1SyncBasicPortDsEntry 8 }
-- .2.15.9.1.8

ieee1588L1SyncBasicPortDsL1SyncReceiptTimeout OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    UNITS        "log2 intervals"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The value specifies the number of elapsed L1SyncIntervals that
        must pass without reception of the L1_SYNC TLV before the
        L1_SYNC TLV reception timeout occurs."
    REFERENCE    "IEEE 1588-2019: clause L.5.2.7, L.4.7"
    ::= { ieee1588L1SyncBasicPortDsEntry 9 }
-- .2.15.9.1.9

ieee1588L1SyncBasicPortDsL1SyncLinkAlive OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "True when a L1_SYNC TLV is received at the PTP Port and the PTP
        Port is configured with ieee1588L1SyncBasicPortDsL1SyncEnabled
        True. It is set to false when the L1_SYNC TLV reception timeout
        occurs."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.1"
    ::= { ieee1588L1SyncBasicPortDsEntry 10 }
-- .2.15.9.1.10

ieee1588L1SyncBasicPortDsIsTxCoherent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be set to True when the L1Sync Port is a
        transmit coherent port."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.2, L.2.3"
    ::= { ieee1588L1SyncBasicPortDsEntry 11 }
-- .2.15.9.1.11

ieee1588L1SyncBasicPortDsIsRxCoherent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be set to True when the L1Sync Port is a
        receive coherent port."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.3, L.2.4"
    ::= { ieee1588L1SyncBasicPortDsEntry 12 }
-- .2.15.9.1.12

ieee1588L1SyncBasicPortDsIsCongruent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be set to True when the PTP Port is a congruent
        port."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.4, L.2.5"
    ::= { ieee1588L1SyncBasicPortDsEntry 13 }
-- .2.15.9.1.13

ieee1588L1SyncBasicPortDsL1SyncState OBJECT-TYPE
    SYNTAX       Ieee1588L1SyncState
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be the value of the current state of the L1Sync
        state machine associated with this L1Sync port within PTP
        Instance."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.5"
    ::= { ieee1588L1SyncBasicPortDsEntry 14 }
-- .2.15.9.1.14

ieee1588L1SyncBasicPortDsPeerTxCoherentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Specifies whether the specific L1Sync port within PTP Instance
        is required to be a transmit coherent port by a peer. As
        indicated in the value of the TCR field of the most recently
        received L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.6"
    ::= { ieee1588L1SyncBasicPortDsEntry 15 }
-- .2.15.9.1.15

ieee1588L1SyncBasicPortDsPeerRxCoherentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Specifies whether the specific L1Sync port within PTP Instance
        is required to be a receive coherent port by a peer. As
        indicated in the value of the RCR field of the most recently
        received L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.7"
    ::= { ieee1588L1SyncBasicPortDsEntry 16 }
-- .2.15.9.1.16

ieee1588L1SyncBasicPortDsPeerCongruentIsRequired OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Specifies whether the specific L1Sync port within PTP Instance
        is required to be a congruent port by a peer. As indicated in
        the value of the CR field of the most recently received L1_SYNC
        TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.8"
    ::= { ieee1588L1SyncBasicPortDsEntry 17 }
-- .2.15.9.1.17

ieee1588L1SyncBasicPortDsPeerIsTxCoherent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "True when the peer L1Sync port within PTP Instance is a
        transmit coherent port. As indicated in the value of the ITC
        field of the most recently received L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.9"
    ::= { ieee1588L1SyncBasicPortDsEntry 18 }
-- .2.15.9.1.18

ieee1588L1SyncBasicPortDsPeerIsRxCoherent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "True when the peer L1Sync port within PTP Instance is a receive
        coherent port. As indicated in the value of the IRC field of the
        most recently received L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.10"
    ::= { ieee1588L1SyncBasicPortDsEntry 19 }
-- .2.15.9.1.19

ieee1588L1SyncBasicPortDsPeerIsCongruent OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "True when the peer L1Sync port within PTP Instance is a
        congruent port. As indicated in the value of the IC field of the
        most recently received L1_SYNC TLV."
    REFERENCE    "IEEE 1588-2019: clause L.5.3.11"
    ::= { ieee1588L1SyncBasicPortDsEntry 20 }
-- .2.15.9.1.20

ieee1588L1SyncOptParamsPortDsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588L1SyncOptParamsPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The L1SyncOptParamsPortDS data set for a specific PTP Port
        within PTP Instance. Provides data for operation of the optional
        layer-1 based synchronization performance enhancement feature.
        This data is optional when the feature is supported."
    REFERENCE    "IEEE 1588-2019: clause L.8.4"
    ::= { ieee1588PortList 10 }
-- .2.15.10

ieee1588L1SyncOptParamsPortDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588L1SyncOptParamsPortDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry providing attributes of L1SyncOptParamsPortDS data set
        for a specific PTP Port within PTP Instance."
    INDEX        {
        ieee1588InstanceIndex,
        ieee1588PortIndex }
    ::= { ieee1588L1SyncOptParamsPortDsTable 1 }
-- .2.15.10.1


Ieee1588L1SyncOptParamsPortDsEntry    ::= SEQUENCE {
        ieee1588L1SyncOptParamsPortDsTimestampsCorrectedTx TruthValue,
        ieee1588L1SyncOptParamsPortDsPhaseOffsetTxValid TruthValue,
        ieee1588L1SyncOptParamsPortDsFrequencyOffsetTxValid TruthValue,
        ieee1588L1SyncOptParamsPortDsPhaseOffsetTx Ieee1588TimeInterval,
        ieee1588L1SyncOptParamsPortDsPhaseOffsetTxTsSecondsField
                                             UInteger48,
        ieee1588L1SyncOptParamsPortDsPhaseOffsetTxTsNanosecondsField
                                             Unsigned32,
        ieee1588L1SyncOptParamsPortDsFrequencyOffsetTx
                                             Ieee1588TimeInterval,
        ieee1588L1SyncOptParamsPortDsFrequencyOffsetTxTsSecondsField
                                             UInteger48,
        ieee1588L1SyncOptParamsPortDsFrequencyOffsetTxTsNanosecondsField
                                             Unsigned32
        }

ieee1588L1SyncOptParamsPortDsTimestampsCorrectedTx OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value specifies whether the L1Sync port within PTP
        Instance shall correct the transmitted egress timestamps with
        the known value of the phase offset, as indicated in the Link
        Reference Model defined in IEEE1588-2019 L.3."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.2.1"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 3 }
-- .2.15.10.1.3

ieee1588L1SyncOptParamsPortDsPhaseOffsetTxValid OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be True if and only if the values of the
        transmission phase offset parameters (provided by
        L1SyncOptParamsPortDsPhaseOffsetTx and
        L1SyncOptParamsPortDsPhaseOffsetTxTimestamp) are valid."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.1"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 4 }
-- .2.15.10.1.4

ieee1588L1SyncOptParamsPortDsFrequencyOffsetTxValid OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The value shall be True if and only if the values of the
        transmission frequency offset parameters (provided by
        L1SyncOptParamsPortDsFrequencyOffsetTx and
        L1SyncOptParamsPortDsFrequencyOffsetTxTimestamp) are valid."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.2"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 5 }
-- .2.15.10.1.5

ieee1588L1SyncOptParamsPortDsPhaseOffsetTx OBJECT-TYPE
    SYNTAX       Ieee1588TimeInterval
    UNITS        "scaled nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Transmission phase offset, which is the time difference between
        the significant instant with which the passage of the message
        timestamp point through the reference plane is aligned, and the
        time represented by the captured timestamp of this passage of
        the message."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.3, L.8.2"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 6 }
-- .2.15.10.1.6

ieee1588L1SyncOptParamsPortDsPhaseOffsetTxTsSecondsField OBJECT-TYPE
    SYNTAX       UInteger48
    UNITS        "seconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Transmission phase offset timestamp for the associated
        transmission phase offset. Expressed in the timescale in use.
        Seconds part of a timestamp."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.4, L.8.2"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 7 }
-- .2.15.10.1.7

ieee1588L1SyncOptParamsPortDsPhaseOffsetTxTsNanosecondsField OBJECT-TYPE
    SYNTAX       Unsigned32(0..999999999)
    UNITS        "nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Transmission phase offset timestamp for the associated
        transmission phase offset. Expressed in the timescale in use.
        Nanoseconds part of a timestamp."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.4, L.8.2"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 8 }
-- .2.15.10.1.8

ieee1588L1SyncOptParamsPortDsFrequencyOffsetTx OBJECT-TYPE
    SYNTAX       Ieee1588TimeInterval
    UNITS        "scaled nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Transmission frequency offset, multiplied by one second.
        Transmission frequency offset is the known rate of change of the
        transmission phase offset."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.5"
    ::= { ieee1588L1SyncOptParamsPortDsEntry 9 }
-- .2.15.10.1.9

ieee1588L1SyncOptParamsPortDsFrequencyOffsetTxTsSecondsField OBJECT-TYPE
    SYNTAX       UInteger48
    UNITS        "seconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Transmission frequency offset timestamp for the associated
        transmission frequency offset. Expressed in the timescale in
        use. Seconds part of a timestamp."
    REFERENCE    "IEEE 1588-2019: clause L.8.4.3.6"
