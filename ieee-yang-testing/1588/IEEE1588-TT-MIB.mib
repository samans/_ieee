        ieee1588UnicastDiscoveryPortDsMaxTableSize Unsigned32,
        ieee1588UnicastDiscoveryPortDsLogQueryInterval
                                             Ieee1588LogTimeInterval
        }

ieee1588UnicastDiscoveryPortDsMaxTableSize OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Maximum number of elements permitted in the
        ieee1588UnicastDiscoveryPortDsPortAddressTable for a specific
        PTP Port within PTP Instance."
