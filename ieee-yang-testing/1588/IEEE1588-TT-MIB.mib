    DESCRIPTION
        "The clockQuality.clockAccuracy attribute of the Grandmaster PTP
        Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.7"
    ::= { ieee1588ParentDsEntry 9 }
-- .2.3.1.9

ieee1588ParentDsGrandmasterClockQualityOffsetScaledLogVariance
                                                             OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The clockQuality.offsetScaledLogVariance attribute of the
        Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.7"
    ::= { ieee1588ParentDsEntry 10 }
-- .2.3.1.10

ieee1588ParentDsGrandmasterPriority1 OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The priority1 attribute of the Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.8"
    ::= { ieee1588ParentDsEntry 11 }
-- .2.3.1.11

ieee1588ParentDsGrandmasterPriority2 OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The priority2 attribute of the Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.9"
    ::= { ieee1588ParentDsEntry 12 }
-- .2.3.1.12

ieee1588ParentDsProtocolAddressNetworkProtocol OBJECT-TYPE
    SYNTAX       Ieee1588NetworkProtocol
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This value indicates the type of the protocol address of the
        PTP Port that issues the Sync messages used in synchronizing
        this PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.10, 5.3.6"
    ::= { ieee1588ParentDsEntry 13 }
-- .2.3.1.13

ieee1588ParentDsProtocolAddressAddressLength OBJECT-TYPE
    SYNTAX       Unsigned32(1..16)
    UNITS        "octets"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This value indicates the length in Octets of the address of the
        PTP Port that issues the Sync messages used in synchronizing
        this PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.10, 5.3.6"
    ::= { ieee1588ParentDsEntry 14 }
-- .2.3.1.14

ieee1588ParentDsProtocolAddressAddressField OBJECT-TYPE
    SYNTAX       PhysAddress(SIZE (1..16))
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This value indicates the address of the PTP Port that issues
        the Sync messages used in synchronizing this PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.10, 5.3.6"
    ::= { ieee1588ParentDsEntry 15 }
-- .2.3.1.15

ieee1588ParentDsSynchronizationUncertain OBJECT-TYPE
