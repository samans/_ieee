ieee1588Notifications OBJECT IDENTIFIER ::= { ieee1588MibV1 0 }

--=====================================================================
-- Data Sets
--=====================================================================


ieee1588Objects OBJECT IDENTIFIER ::= { ieee1588MibV1 1 }

ieee1588Base OBJECT IDENTIFIER ::= { ieee1588Objects 1 }

ieee1588BaseInstanceTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588BaseInstanceEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This table lists PTP Instances in the PTP Node. This table is
        used to allow for dynamic creation and deletion of PTP Instances
        in implementations that support it."
    REFERENCE    "IEEE 1588-2019: clause 6.1.3, 7.6"
    ::= { ieee1588Base 1 }
-- .1.1

ieee1588BaseInstanceEntry OBJECT-TYPE
    SYNTAX       Ieee1588BaseInstanceEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing a PTP Instance. Each PTP Instance
        represents a distinct instance of PTP implementation (i.e.
        distinct Ordinary Clock, Boundary Clock, or Transparent Clock),
        maintaining a distinct time. PTP Instances may be created or
        deleted dynamically in implementations that support dynamic
        create/delete."
    REFERENCE    "IEEE 1588-2019: clause 6.1.3, 7.6"
    INDEX        {
        ieee1588InstanceIndex }
    ::= { ieee1588BaseInstanceTable 1 }
-- .1.1.1


Ieee1588BaseInstanceEntry    ::= SEQUENCE {
        ieee1588InstanceIndex                   Unsigned32,
        ieee1588BaseInstanceRowStatus           RowStatus,
        ieee1588BaseInstanceName                SnmpAdminString
        }

ieee1588InstanceIndex OBJECT-TYPE
    SYNTAX       Unsigned32(1..65534)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An numeric index identifying a specific PTP Instance. A numeric
        index is used instead of ClockIdentify to identify a specific
        PTP Instance."
    ::= { ieee1588BaseInstanceEntry 1 }
-- .1.1.1.1

ieee1588BaseInstanceRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This object is used to create and delete PTP Instances."
    ::= { ieee1588BaseInstanceEntry 2 }
-- .1.1.1.2

ieee1588BaseInstanceName OBJECT-TYPE
    SYNTAX       SnmpAdminString
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "Name for identification of a PTP Instance. Used only for
        management."
    ::= { ieee1588BaseInstanceEntry 3 }
-- .1.1.1.3

ieee1588BasePortTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588BasePortEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This table contains mapping between ieee1588PortIndex
        (associated with a PTP Port or Common Service Port) used within
        a PTP Instance into a physical port index."
    ::= { ieee1588Base 2 }
-- .1.2

ieee1588BasePortEntry OBJECT-TYPE
    SYNTAX       Ieee1588BasePortEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing a PTP Port"
    INDEX        {
        ieee1588InstanceIndex,
        ieee1588PortIndex }
    ::= { ieee1588BasePortTable 1 }
-- .1.2.1


Ieee1588BasePortEntry    ::= SEQUENCE {
        ieee1588PortIndex                       Unsigned32,
        ieee1588BasePortRowStatus               RowStatus,
        ieee1588BasePortIfIndex                 InterfaceIndexOrZero,
        ieee1588BasePortIfIndexOid              Ieee1588IfIndexOid
        }

ieee1588PortIndex OBJECT-TYPE
    SYNTAX       Unsigned32(1..65534)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An index that is unique per PTP Port within the PTP Instance,
        applicable to the management context only (i.e. not used in PTP
        Messages). NOTE: this number might not be equivalent to the
        portNumber attribute (portDS.portIdentity.portNumber data set
        member)."
    REFERENCE    "IEEE 1588-2019: clause 7.5.2.3"
    ::= { ieee1588BasePortEntry 2 }
-- .1.2.1.2

ieee1588BasePortRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This object is used to create and delete PTP Ports."
    ::= { ieee1588BasePortEntry 3 }
-- .1.2.1.3

ieee1588BasePortIfIndex OBJECT-TYPE
    SYNTAX       InterfaceIndexOrZero
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This value can be used to identify the interface used by the
        respective PTP Port.
        If this value is non-zero, it refers to the ifIndex object of
        the used interface in the ifTable (IF-MIB). If this value is
        zero, the ifIndex object is not used and
        ieee1588BasePortIfIndexOid can reference the interface used by
        this PTP Port. If this value is zero and Ieee1588IfIndexOid is
        zeroDotZero or is not defined then PTP Port has no defined
        underlying interface."
    REFERENCE
          "IEEE 1588-2019 as amended by IEEE Std 1588e: clause 15.3.3.8"
    ::= { ieee1588BasePortEntry 4 }
-- .1.2.1.4

ieee1588BasePortIfIndexOid OBJECT-TYPE
    SYNTAX       Ieee1588IfIndexOid
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "This OID can be used to identify the interface of the
        respective PTP Port, if the value of ieee1588BasePortIfIndex is
        zero. The value of this object references the index of the used
        interface. If ieee1588BasePortIfIndex is set to a non-zero
        value, the value of this OID is not used."
    REFERENCE
          "IEEE 1588-2019 as amended by IEEE Std 1588e: clause 15.3.3.8"
    ::= { ieee1588BasePortEntry 5 }
-- .1.2.1.5

-- Maps enumerate values defined in the standard into OIDs
