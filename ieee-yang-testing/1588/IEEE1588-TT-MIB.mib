        ieee1588ParentDsParentPortIdentityClockIdentity
                                             Ieee1588ClockIdentity,
        ieee1588ParentDsParentPortIdentityPortNumber Unsigned32,
        ieee1588ParentDsParentStats             TruthValue,
        ieee1588ParentDsObservedParentOffsetScaledLogVariance
                                             Unsigned32,
        ieee1588ParentDsObservedParentClockPhaseChangeRate Integer32,
        ieee1588ParentDsGrandmasterIdentity     Ieee1588ClockIdentity,
        ieee1588ParentDsGrandmasterClockQualityClockClass
                                             Ieee1588ClockClass,
        ieee1588ParentDsGrandmasterClockQualityClockAccuracy
                                             Ieee1588ClockAccuracy,
        ieee1588ParentDsGrandmasterClockQualityOffsetScaledLogVariance
                                             Unsigned32,
        ieee1588ParentDsGrandmasterPriority1    Unsigned32,
        ieee1588ParentDsGrandmasterPriority2    Unsigned32,
        ieee1588ParentDsProtocolAddressNetworkProtocol
                                             Ieee1588NetworkProtocol,
        ieee1588ParentDsProtocolAddressAddressLength Unsigned32,
        ieee1588ParentDsProtocolAddressAddressField PhysAddress,
        ieee1588ParentDsSynchronizationUncertain TruthValue
        }

ieee1588ParentDsParentPortIdentityClockIdentity OBJECT-TYPE
    SYNTAX       Ieee1588ClockIdentity
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The clockIdentity of the Parent PTP Instance that issues the
        Sync messages."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.2"
    ::= { ieee1588ParentDsEntry 2 }
-- .2.3.1.2

ieee1588ParentDsParentPortIdentityPortNumber OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The number of the PTP Port on the parent (timeTransmitter) PTP
        Instance that issues the Sync Messages used in synchronizing the
        Local PTP Clock of this PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.2"
    ::= { ieee1588ParentDsEntry 3 }
-- .2.3.1.3

ieee1588ParentDsParentStats OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "True if all of the following conditions are satisfied:
        - The PTP Instance has a port in the TIME_RECEIVER state.
        - The PTP Instance has computed statistically valid estimates of
        the ieee1588ParentDsObservedParentOffsetScaledLogVariance and
        the ieee1588ParentDsObservedParentClockPhaseChangeRate.
        Otherwise false."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.3"
    ::= { ieee1588ParentDsEntry 4 }
-- .2.3.1.4

ieee1588ParentDsObservedParentOffsetScaledLogVariance OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Estimate of the variance of the phase offset of the Local PTP
        Clock of the Parent PTP Instance as measured with respect to the
        Local PTP Clock in the TimeReceiver PTP Instance. This
        measurement is optional, but if not made, the value of
        ieee1588ParentDsParentStats shall be false."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.4"
    ::= { ieee1588ParentDsEntry 5 }
-- .2.3.1.5

ieee1588ParentDsObservedParentClockPhaseChangeRate OBJECT-TYPE
    SYNTAX       Integer32
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Estimate of the phase change rate of the Local PTP Clock of the
        Parent PTP Instance as measured by the TimeReceiver PTP Instance
        using its Local PTP Clock.
        If the estimate exceeds the capacity of its data type, this
        value shall be set to 7FFF FFFF (base 16) or 8000 0000 (base
        16), as appropriate. A positive sign indicates that the phase
        change rate in the Parent PTP Instance is greater than that in
        the TimeReceiver PTP Instance. The measurement of this value is
        optional, but if not measured, the value of
        parent-ds/parent-stats shall be false."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.5"
    ::= { ieee1588ParentDsEntry 6 }
-- .2.3.1.6

ieee1588ParentDsGrandmasterIdentity OBJECT-TYPE
    SYNTAX       Ieee1588ClockIdentity
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The clockIdentity of the Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3.6"
    ::= { ieee1588ParentDsEntry 7 }
-- .2.3.1.7

ieee1588ParentDsGrandmasterClockQualityClockClass OBJECT-TYPE
