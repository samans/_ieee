ieee1588DefaultDsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588DefaultDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The defaultDS data set for a specific PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1"
    ::= { ieee1588Instances 1 }
-- .2.1

ieee1588DefaultDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588DefaultDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing the defaultDS data set for a specific PTP
        Instance."
    INDEX        {
        ieee1588InstanceIndex }
    ::= { ieee1588DefaultDsTable 1 }
-- .2.1.1


Ieee1588DefaultDsEntry    ::= SEQUENCE {
        ieee1588DefaultDsTwoStepFlag            TruthValue,
        ieee1588DefaultDsClockIdentity          Ieee1588ClockIdentity,
        ieee1588DefaultDsNumberPorts            Unsigned32,
        ieee1588DefaultDsClockQualityClockClass Ieee1588ClockClass,
        ieee1588DefaultDsClockQualityClockAccuracy
                                             Ieee1588ClockAccuracy,
        ieee1588DefaultDsClockQualityOffsetScaledLogVariance Unsigned32,
        ieee1588DefaultDsPriority1              Unsigned32,
        ieee1588DefaultDsPriority2              Unsigned32,
        ieee1588DefaultDsDomainNumber           Unsigned32,
        ieee1588DefaultDsTimeReceiverOnly       TruthValue,
        ieee1588DefaultDsSdoId                  Unsigned32,
        ieee1588DefaultDsCurrentTimeSecondsField UInteger48,
        ieee1588DefaultDsCurrentTimeNanosecondsField Unsigned32,
        ieee1588DefaultDsInstanceEnable         TruthValue,
        ieee1588DefaultDsExternalPortConfigurationEnabled TruthValue,
        ieee1588DefaultDsMaxStepsRemoved        Unsigned32,
        ieee1588DefaultDsInstanceType           Ieee1588InstanceType
        }

ieee1588DefaultDsTwoStepFlag OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       deprecated
    DESCRIPTION
        "When set to true, the PTP Instance is two-step, otherwise the
        PTP Instance is one-step. This data set member is no longer
        used. However, the twoStepFlag of the PTP common header is used.
        One step or two step egress behavior is allowed to be specified
        per PTP Port, or per PTP Instance. Management of the one/two
        step egress behavior of a PTP Port is not provided by this
        standard, but can be specified as extensions to the data sets by
        a PTP Profile or a product specification."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.2.1"
    ::= { ieee1588DefaultDsEntry 2 }
-- .2.1.1.2

ieee1588DefaultDsClockIdentity OBJECT-TYPE
    SYNTAX       Ieee1588ClockIdentity
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This value indicates the clockIdentity of the PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.2.2"
    ::= { ieee1588DefaultDsEntry 3 }
-- .2.1.1.3

ieee1588DefaultDsNumberPorts OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This value indicates the number of PTP Ports on the PTP
        Instance. For an Ordinary Clock, this value shall be 1."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.2.3"
    ::= { ieee1588DefaultDsEntry 4 }
-- .2.1.1.4

ieee1588DefaultDsClockQualityClockClass OBJECT-TYPE
    SYNTAX       Ieee1588ClockClass
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the clockClass attribute of an Ordinary
        Clock or Boundary Clock and denotes the traceability,
        synchronization state and expected performance of the time or
        frequency distributed by the Grandmaster PTP Instance. The
        clockClass is one of the attributes that characterizes a PTP
        Instance for the purpose of the Best TimeTransmitter Clock
        Algorithm (BTCA)."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.3.1.2"
    ::= { ieee1588DefaultDsEntry 5 }
-- .2.1.1.5

ieee1588DefaultDsClockQualityClockAccuracy OBJECT-TYPE
    SYNTAX       Ieee1588ClockAccuracy
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the accuracy of the clock (Local PTP Clock
        of the PTP Instance)."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.3.1.3"
    ::= { ieee1588DefaultDsEntry 6 }
-- .2.1.1.6

ieee1588DefaultDsClockQualityOffsetScaledLogVariance OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the stability of the Local PTP Clock of
        the PTP Instance. It provides an estimate of the variations of
        the Local PTP Clock when it is not synchronized to another Local
        PTP Clock using the protocol."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.3.1.4"
    ::= { ieee1588DefaultDsEntry 7 }
-- .2.1.1.7

ieee1588DefaultDsPriority1 OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the priority1 of the PTP Instance. Lower
        values take precedence."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.4.1"
    ::= { ieee1588DefaultDsEntry 8 }
-- .2.1.1.8

ieee1588DefaultDsPriority2 OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the priority2 of the PTP Instance. Lower
        values take precedence."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.4.2"
    ::= { ieee1588DefaultDsEntry 9 }
-- .2.1.1.9

ieee1588DefaultDsDomainNumber OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the domainNumber of the PTP Instance.
        A domain consists of one or more PTP Instances communicating
        with each other as defined by the protocol. A domain shall
        define the scope of PTP message communication, state,
        operations, data sets, and timescale. Therefore, each domain
        represents a distinct time.
        Within a PTP Network, a domain is identified by two data set
        members: domainNumber and sdoId. The domainNumber is the primary
        mechanism for end users and system integrators to isolate the
        operation of a PTP Instance from PTP messages used in other
        domains. The value of the domainNumber shall be configurable to
        values permitted in IEEE Std 1588, unless the allowed values are
        further restricted by the applicable PTP Profile."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.4.3"
    ::= { ieee1588DefaultDsEntry 10 }
-- .2.1.1.10

ieee1588DefaultDsTimeReceiverOnly OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value shall be true if the PTP Instance is a
        timeReceiver-only PTP Instance (false for
        non-timeReceiver-only).
        The timeReceiver-only member can be true for Ordinary Clocks
        only. When timeReceiver-only is true, the PTP Instance
        implements special behavior in the context of the state machines
        that determine port-state."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.4.4"
    ::= { ieee1588DefaultDsEntry 11 }
-- .2.1.1.11

ieee1588DefaultDsSdoId OBJECT-TYPE
    SYNTAX       Unsigned32(0..4095)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the sdoId of the PTP Instance.
        A domain consists of one or more PTP Instances communicating
        with each other as defined by the protocol. A domain shall
        define the scope of PTP message communication, state,
        operations, data sets, and timescale. Therefore, each domain
        represents a distinct time.
        Within a PTP Network, a domain is identified by two data set
        members: domainNumber and sdoId. The sdoId of a domain is a
        12-bit integer in the closed range 0 to 4095. The sdoId member
        is the primary mechanism for providing isolation of PTP
        Instances operating a PTP Profile specified by a Standards
        Development Organization (SDO), from other PTP Instances
        operating under a PTP Profile specified by a different SDO."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.4.5"
    ::= { ieee1588DefaultDsEntry 12 }
-- .2.1.1.12

ieee1588DefaultDsCurrentTimeSecondsField OBJECT-TYPE
    SYNTAX       UInteger48
    UNITS        "seconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value represents the integer portion of the timestamp in
        units of seconds, a positive time with respect to the epoch.
        For management read, this member shall return the current value
        of the PTP Instance Time. When management write is supported,
        this member shall set the PTP Instance Time. Time originates in
        the Grandmaster PTP Instance and is distributed by PTP to other
        PTP Instances in the domain.
        NOTE 1 - The time in the Grandmaster PTP Instance is normally
        determined by interacting with a primary reference, e.g., GPS,
        by means outside the scope of this standard.
        NOTE 2 - When this member is used to set time in a PTP Instance
        other than the Grandmaster PTP Instance, the PTP Node can return
        a management error.
        NOTE 3 - If the time is set in a PTP Instance other than the
        Grandmaster PTP Instance, it will be overwritten by the
        operation of the protocol and will therefore exist only as a
        transient."
    REFERENCE    "IEEE 1588-2019: clause 5.3.3, 8.2.1.5.1"
    ::= { ieee1588DefaultDsEntry 13 }
-- .2.1.1.13

ieee1588DefaultDsCurrentTimeNanosecondsField OBJECT-TYPE
    SYNTAX       Unsigned32(0..999999999)
    UNITS        "nanoseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value represents the fractional portion of the timestamp
        in units of nanoseconds, a positive time with respect to the
        epoch. This value is always less than 10^9.
        For management read, this member shall return the current value
        of the PTP Instance Time.
        When management write is supported, this member shall set the
        PTP Instance Time.
        Time originates in the Grandmaster PTP Instance and is
        distributed by PTP to other PTP Instances in the domain.
        NOTE 1 - The time in the Grandmaster PTP Instance is normally
        determined by interacting with a primary reference, e.g., GPS,
        by means outside the scope of this standard.
        NOTE 2 - When this member is used to set time in a PTP Instance
        other than the Grandmaster PTP Instance, the PTP Node can return
        a management error.
        NOTE 3 - If the time is set in a PTP Instance other than the
        Grandmaster PTP Instance, it will be overwritten by the
        operation of the protocol and will therefore exist only as a
        transient."
    REFERENCE    "IEEE 1588-2019: clause 5.3.3, 8.2.1.5.1"
    ::= { ieee1588DefaultDsEntry 14 }
-- .2.1.1.14

ieee1588DefaultDsInstanceEnable OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates if the PTP Instance is enabled for PTP
        operation. When management write is supported:
        - Write of the value true shall cause the PTP Instance to
        initialize, only if the value was previously false.
        - Write of the value false shall immediately disable operation
        of the PTP Instance (i.e. analogous to power off).
        If this OID is not supported, the PTP Instance shall be
        specified-by-design to be enabled (true)."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.5.2"
    ::= { ieee1588DefaultDsEntry 15 }
-- .2.1.1.15

ieee1588DefaultDsExternalPortConfigurationEnabled OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value determines whether the external port configuration
        option is in the disabled state (false) or enabled state (true).
        When this value is false, each PTP Port's state is determined by
        PTP state machines, including the Best TimeTransmitter Clock
        Algorithm (BTCA). When this value is true, each PTP Port's state
        is configured externally, and PTP state machines are effectively
        disabled. External configuration of PTP Port state can be
        accomplished using the
        ieee1588ExternalPortConfigurationPortDsDesiredState member of
        the PTP Port."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.5.3"
    ::= { ieee1588DefaultDsEntry 16 }
-- .2.1.1.16

ieee1588DefaultDsMaxStepsRemoved OBJECT-TYPE
    SYNTAX       Unsigned32(0..255)
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The maximum value of stepsRemoved that qualifies an Announce
        message for consideration by the Best TimeTransmitter Clock
        Algorithm (BTCA). If the value of stepsRemoved of an Announce
        message is greater than or equal to the value of this
        max-steps-removed value, the Announce message is not considered
        in the operation of the BTCA.
        The value shall be in the closed range 2 to 255."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.5.4"
    ::= { ieee1588DefaultDsEntry 17 }
-- .2.1.1.17

ieee1588DefaultDsInstanceType OBJECT-TYPE
    SYNTAX       Ieee1588InstanceType
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This value indicates the type of PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.1.5.5"
    ::= { ieee1588DefaultDsEntry 18 }
-- .2.1.1.18

ieee1588CurrentDsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588CurrentDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The currentDS data set for a specific PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.2"
    ::= { ieee1588Instances 2 }
-- .2.2

ieee1588CurrentDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588CurrentDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing currentDS data set in a PTP Instance for
        operation of the protocol."
    INDEX        {
        ieee1588InstanceIndex }
    ::= { ieee1588CurrentDsTable 1 }
-- .2.2.1


Ieee1588CurrentDsEntry    ::= SEQUENCE {
        ieee1588CurrentDsStepsRemoved           Unsigned32,
        ieee1588CurrentDsOffsetFromTimeTransmitter Ieee1588TimeInterval,
        ieee1588CurrentDsMeanDelay              Ieee1588TimeInterval,
        ieee1588CurrentDsSyncUncertain          TruthValue
        }

ieee1588CurrentDsStepsRemoved OBJECT-TYPE
    SYNTAX       Unsigned32(0..65535)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The number of PTP Communication Paths traversed between this
        PTP Instance and the Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.2.2"
    ::= { ieee1588CurrentDsEntry 2 }
-- .2.2.1.2

ieee1588CurrentDsOffsetFromTimeTransmitter OBJECT-TYPE
    SYNTAX       Ieee1588TimeInterval
    UNITS        "scaled nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The current value of the time difference between a
        TimeTransmitter PTP Instance and a TimeReceiver PTP Instance as
        computed by the TimeReceiver PTP Instance. NOTE - When a PTP
        Profile requires a Boundary Clock to transfer offset information
        internally from TimeReceiver PTP Port to TimeTransmitter PTP
        Port(s), this value effectively returns the offset from the
        Grandmaster PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.2.3"
    ::= { ieee1588CurrentDsEntry 3 }
-- .2.2.1.3

ieee1588CurrentDsMeanDelay OBJECT-TYPE
    SYNTAX       Ieee1588TimeInterval
    UNITS        "scaled nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The current value of the mean propagation time between a
        TimeTransmitter PTP Instance and a TimeReceiver PTP Instance as
        computed by the TimeReceiver PTP Instance. If the PTP Instance
        has no PTP Port in timeReceiver or uncalibrated state, this
        returns zero. Otherwise, the timeReceiver PTP port returns this
        value depending on its delay-mechanism:
        e2e: mean propagation time over the PTP Communication Path, i.e.
        <meanPathDelay>
        p2p or common_p2p: mean propagation time over the PTP Link, i.e.
        <meanLinkDelay>
        disabled or special: zero"
    REFERENCE    "IEEE 1588-2019: clause 8.2.2.4"
    ::= { ieee1588CurrentDsEntry 4 }
-- .2.2.1.4

ieee1588CurrentDsSyncUncertain OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "This boolean is true when synchronization is uncertain (e.g.,
        not within specification) in either the Parent PTP Port or this
        PTP Instance. The value is copied from a received Announce
        message to transmitted Announce message, such that it reflects
        uncertain synchronization from this PTP Instance to the
        Grandmaster. Performance metrics for determining uncertainty are
        specified by the applicable PTP Profile."
    REFERENCE    "IEEE 1588-2019: clause 8.2.2.5"
    ::= { ieee1588CurrentDsEntry 5 }
-- .2.2.1.5

ieee1588ParentDsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Ieee1588ParentDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The parentDS data set for a specific PTP Instance."
    REFERENCE    "IEEE 1588-2019: clause 8.2.3"
    ::= { ieee1588Instances 3 }
-- .2.3

ieee1588ParentDsEntry OBJECT-TYPE
    SYNTAX       Ieee1588ParentDsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An Entry representing data learned from the parent of a PTP
        Instance (i.e. TimeTransmitter PTP Port at the other end of the
