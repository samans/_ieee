    ::= { ieee1588PortDsEntry 6 }
-- .2.15.1.1.6

ieee1588PortDsMeanLinkDelay OBJECT-TYPE
    SYNTAX       Ieee1588TimeInterval
    UNITS        "scaled nanoseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
