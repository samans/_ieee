    ::= { ieee8021AsV3MIBObjects 6 }

ieee8021AsV3PathTraceDSEntry   OBJECT-TYPE
    SYNTAX      Ieee8021AsV3PathTraceDSEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Path Trace Data Set for a specific PTP Instance."
