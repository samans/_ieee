-- =============================================================
-- The Common Mean Link Delay Service Link Port Parameter
-- Statistics Data Set provides counters associated with Link
-- Port capabilities at a given time-aware system.
-- =============================================================

ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "For every Link Port of the Common Mean Link Delay Service of a
         time-aware system, the following cmldsLinkPortStatisticsDS
         provides counters. The number of such statistics sets is the
         same as the value of cmldsDefaultDS.numberLinkPorts."
    REFERENCE   "14.17"
    ::= { ieee8021AsV3MIBObjects 19 }

ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry   OBJECT-TYPE
    SYNTAX      Ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "For every Link Port of the Common Mean Link Delay Service of a
         time-aware system, the following cmldsLinkPortStatisticsDS
         provides counters. The number of such statistics sets is the
         same as the value of cmldsDefaultDS.numberLinkPorts."
    INDEX { ieee8021AsV3BridgeBasePort,
            ieee8021AsV3CmldsLinkPortStatDSIndex }
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSTable 1 }

Ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry ::=
  SEQUENCE {
    ieee8021AsV3CmldsLinkPortStatDSIndex           InterfaceIndexOrZero,
    ieee8021AsV3CmldsLinkPortStatDSRxPdelayRequestCount            Counter32,
    ieee8021AsV3CmldsLinkPortStatDSRxPdelayRspCount           Counter32,
    ieee8021AsV3CmldsLinkPortStatDSRxPdelayRspFollowUpCount   Counter32,
    ieee8021AsV3CmldsLinkPortStatDSRxPtpPacketDiscardCount         Counter32,
    ieee8021AsV3CmldsLinkPortStatDSPdelayAllowedLostRspExceededCount  Counter32,
    ieee8021AsV3CmldsLinkPortStatDSTxPdelayRequestCount            Counter32,
    ieee8021AsV3CmldsLinkPortStatDSTxPdelayRspCount           Counter32,
    ieee8021AsV3CmldsLinkPortStatDSTxPdelayRspFollowUpCount   Counter32
      }

ieee8021AsV3CmldsLinkPortStatDSIndex OBJECT-TYPE
    SYNTAX      InterfaceIndexOrZero
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An index to identify an entry in the Common Mean Link
         Port Statistics Data Set."
    REFERENCE   "14.17"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 1 }

ieee8021AsV3CmldsLinkPortStatDSRxPdelayRequestCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Req message is
         received."
    REFERENCE   "14.17.2"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 2 }

ieee8021AsV3CmldsLinkPortStatDSRxPdelayRspCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Resp message is
         received."
    REFERENCE   "14.17.3"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 3 }

ieee8021AsV3CmldsLinkPortStatDSRxPdelayRspFollowUpCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Resp_Follow_Up
         message is received."
    REFERENCE   "14.17.4"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 4 }

ieee8021AsV3CmldsLinkPortStatDSRxPtpPacketDiscardCount
 OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a PTP message of the
         Common Mean Link Delay Service is discarded, caused by the
         occurrence of any of the conditions given in 14.17.5."
    REFERENCE   "14.17.5"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 5 }

ieee8021AsV3CmldsLinkPortStatDSPdelayAllowedLostRspExceededCount
 OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time the value of the variable
         lostResponses exceeds the value of the variable
         allowedLostResponses, in the RESET state of the
         MDPdelayReq state machine."
    REFERENCE   "14.17.6"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 6 }

ieee8021AsV3CmldsLinkPortStatDSTxPdelayRequestCount
 OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Req message is
         transmitted."
    REFERENCE   "14.17.7"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 7 }

ieee8021AsV3CmldsLinkPortStatDSTxPdelayRspCount
 OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Resp message is
         transmitted."
    REFERENCE   "14.17.8"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 8 }

ieee8021AsV3CmldsLinkPortStatDSTxPdelayRspFollowUpCount
 OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "A counter that increments every time a Pdelay_Resp_Follow_Up
         message is transmitted."
    REFERENCE   "14.17.9"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortStatDSEntry 9 }

-- =============================================================
-- The Common Mean Link Delay Service Asymmetry Measurement Mode
-- Parameter Data Set represents the capability to enable/disable
-- the Asymmetry Compensation Measurement Procedure on a Link Port
-- (see Annex G).
-- =============================================================

ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The Common Mean Link Delay Service Asymmetry Measurement Mode
         Parameter Data Set represents the capability to enable/disable
         the Asymmetry Compensation Measurement Procedure on a Link Port
         (see Annex G)."
    REFERENCE   "14.18"
    ::= { ieee8021AsV3MIBObjects 20 }

ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry   OBJECT-TYPE
    SYNTAX      Ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table uses
         ieee8021AsV3CmldsAsymmetryMeasurementModeDSAsIndex,
         and corresponds to
         ieee8021AsV3CommonMeanLinkDelayServiceAsymmetryMeasurementModeDSTable
         entry."
    INDEX { ieee8021AsV3BridgeBasePort,
            ieee8021AsV3CmldsAsymMeasurementModeDSAsIndex }
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSTable 1 }

Ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry ::=
     SEQUENCE {
 ieee8021AsV3CmldsAsymMeasurementModeDSAsIndex InterfaceIndexOrZero,
 ieee8021AsV3CmldsAsymMeasurementModeDSAsymMeasurementMode    TruthValue
      }

ieee8021AsV3CmldsAsymMeasurementModeDSAsIndex OBJECT-TYPE
    SYNTAX      InterfaceIndexOrZero
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object identifies the gPTP interface group within
         the system for which this entry contains information. It
         is the value of the instance of the IfIndex object,
         defined in the IF-MIB, for the gPTP interface group
         corresponding to this port, or the value 0 if the port
         has not been bound to an underlying frame source and
         sink.

         For a given media port of a Bridge or an end station,
         there can be one or more PTP Port, and depends whether
         a media port supports point to point link (e.g. IEEE
         802.3 Ethernet) or point to multi-point (e.g. CSN, IEEE
         802.3 EPON) links on the media port."
    REFERENCE   "IEEE Std 802.1AS CommonMeanLinkDelaySvcAsymMeasurementModeParamDS Group PTP Port Index"
  ::= { ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry 1 }

ieee8021AsV3CmldsAsymMeasurementModeDSAsymMeasurementMode
    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value is equal to the value of the Boolean
         asymmetryMeasurementMode (see G.3). For full-duplex
         IEEE 802.3 media, the value is TRUE (1) if an asymmetry
          measurement is being performed for the link attached to
         this Link Port, and FALSE (2) otherwise. For all other
         media, the value shall be FALSE (2) (see 10.2.5.2).
         Setting this managed object causes the Boolean
         allowedFaults to have the same value.
         NOTE: If an asymmetry measurement is being performed
         for a link, asymmetryMeasurementMode must be TRUE (1)
         for the Link Ports at each end of the link.
         There is one Common Mean Link Delay Service Asymmetry
         Measurement Mode Parameter Data Set Table for all PTP
         Instances, per Link Port."
    REFERENCE   "14.18.2"
  ::= { ieee8021AsV3CommonMeanLinkDelayServiceAsymMeasurementModeDSEntry 2 }

