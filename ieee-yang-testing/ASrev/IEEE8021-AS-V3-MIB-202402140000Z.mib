    REFERENCE   "14.16.19"
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortDSEntry 20 }

ieee8021AsV3CmldsLinkPortDSUseMgtSettableComputeMeanLinkDelay OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The managed object is a Boolean that determines the source
         of the value of computeMeanLinkDelay. If the value is
         TRUE (1), the value of computeMeanLinkDelay is set equal
         to the value of mgtSettableComputeMeanLinkDelay. If the
         value of the managed object is FALSE (2), the value of
         currentComputeMeanLinkDelay is determined by the
         LinkDelayIntervalSetting state machine."
    REFERENCE   "14.16.20"
    DEFVAL { false }
    ::= { ieee8021AsV3CommonMeanLinkDelayServiceLinkPortDSEntry 21 }

ieee8021AsV3CmldsLinkPortDSMgtSettableComputeMeanLinkDelay OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
